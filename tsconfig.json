{
  "compilerOptions": {
    "rootDirs": ["./pages", "./src"],
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "skipLibCheck": true
    // "typeRoots": [
    //   "./custom-typings",
    //   "./node_modules/@types"
    // ]
  },
}
